{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "ignoreExportsUsedInFile": {
        "interface": true,
        "type": true
    },
    "ignoreDependencies": [],
    "ignore": ["**/*.config.*", "**/vitest.setup.*", "**/setupTests.*", "**/test-mocks/**"],
    "tags": ["-lintignore"],
    "workspaces": {
        "v6y-apps/front*": {
            "entry": ["src/app/**/*.{ts,tsx}", "next.config.mjs"],
            "project": ["**/*.{ts,tsx}"]
        },
        "v6y-apps/*": {
            "entry": ["src/index.ts"],
            "project": ["src/**/*.{ts,js}"]
        },
        "v6y-libs/*": {}
    }
}
