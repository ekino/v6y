{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignoreExportsUsedInFile": {
    "interface": true,
    "type": true
  },
  "tags": [
    "-lintignore"
  ],
  "workspaces": {
    ".": {
      "entry": ["nx.json"],
      "project": ["nx.json", "package.json"]
    },
    "v6y-apps/*": {
      "entry": ["src/index.ts", "src/main.ts", "src/app/**/*"],
      "project": ["**/*.{ts,tsx,js,jsx}"]
    },
    "v6y-apps/front": {
      "next": {
        "entry": ["next.config.mjs"]
      }
    },
    "v6y-apps/front-bo": {
      "next": {
        "entry": ["next.config.mjs"]
      }
    },
    "v6y-libs/*": {
      "entry": ["src/index.ts"],
      "project": ["**/*.{ts,tsx,js,jsx}"]
    }
  }
}
